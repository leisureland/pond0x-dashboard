import { WalletEvent, WalletStats, Chain } from '@shared/schema';

export interface BlockchainApiResponse {
  events: WalletEvent[];
  stats: WalletStats;
}

export async function fetchEthereumData(address: string): Promise<BlockchainApiResponse> {
  const alchemyKey = process.env.ALCHEMY_API_KEY;
  
  if (!alchemyKey) {
    throw new Error('ALCHEMY_API_KEY not configured');
  }

  try {
    // Fetch transaction history from Alchemy
    const response = await fetch(`https://eth-mainnet.g.alchemy.com/v2/${alchemyKey}`, {
      method: 'POST',
      headers: {
        'Content-Type': 'application/json',
      },
      body: JSON.stringify({
        jsonrpc: '2.0',
        method: 'alchemy_getAssetTransfers',
        params: [{
          fromBlock: '0x0',
          toBlock: 'latest',
          fromAddress: address.toLowerCase(),
          category: ['external', 'internal', 'erc20', 'erc721', 'erc1155'],
          withMetadata: true,
          excludeZeroValue: true,
          maxCount: '0x64'  // 100 transactions
        }],
        id: 1
      })
    });

    if (!response.ok) {
      throw new Error(`Alchemy API error: ${response.status}`);
    }

    const data = await response.json();
    
    if (data.error) {
      throw new Error(`Alchemy API error: ${data.error.message}`);
    }

    const transfers = data.result?.transfers || [];
    
    // Transform Alchemy data to our format with enhanced token information
    const events: WalletEvent[] = transfers.map((transfer: any, index: number) => ({
      id: `eth_${transfer.hash}_${index}`,
      type: transfer.category === 'erc20' ? 'swap' : 'transfer',
      description: `${transfer.category === 'erc20' ? 'Token swap' : 'ETH transfer'}: ${transfer.value || '0'} ${transfer.asset || 'ETH'}`,
      timestamp: new Date(transfer.metadata?.blockTimestamp || Date.now()).getTime(),
      hash: transfer.hash,
      chain: 'eth' as Chain,
      amount: parseFloat(transfer.value || '0'),
      token: transfer.asset || 'ETH',
      tokenSymbol: transfer.asset || 'ETH',
      tokenName: transfer.asset || 'Ethereum',
      contractAddress: transfer.rawContract?.address || undefined,
      fromAddress: transfer.from,
      toAddress: transfer.to
    }));

    // Calculate stats from real data
    const totalSwaps = events.filter(e => e.type === 'swap').length;
    const totalValue = events.reduce((sum, event) => sum + (event.amount || 0), 0);
    const miningRewards = events
      .filter(e => e.description.includes('mining') || e.description.includes('reward'))
      .reduce((sum, event) => sum + (event.amount || 0), 0);

    const stats: WalletStats = {
      totalSwaps,
      totalValue: Math.round(totalValue),
      miningRewards,
      pondProStatus: false,
      pondProExpiry: null
    };

    return { events, stats };

  } catch (error) {
    console.error('Error fetching Ethereum data:', error);
    throw error;
  }
}

export async function fetchSolanaData(address: string): Promise<BlockchainApiResponse> {
  const heliusKey = process.env.HELIUS_API_KEY;
  
  if (!heliusKey) {
    throw new Error('HELIUS_API_KEY not configured');
  }

  try {
    // Use Helius enhanced transaction API for comprehensive token parsing
    const response = await fetch(
      `https://api.helius.xyz/v0/addresses/${address}/transactions?api-key=${heliusKey}&limit=200`,
      { method: 'GET', headers: { 'Content-Type': 'application/json' } }
    );

    if (!response.ok) {
      throw new Error(`Helius API error: ${response.status}`);
    }

    const transactions = await response.json();
    const events: WalletEvent[] = [];
    
    // Process enhanced transaction data for comprehensive token information
    for (const tx of transactions) {
      try {
        let description = tx.description || 'Solana transaction';
        let type: WalletEvent['type'] = 'transfer';
        let amount = 0;
        let token = 'SOL';
        let tokenSymbol = 'SOL';
        let tokenName = 'Solana';
        let contractAddress = undefined;

        // Enhanced token parsing from Helius parsed description
        if (tx.description) {
          const desc = tx.description.toLowerCase();
          
          // Detect transaction type from description
          if (desc.includes('swap') || desc.includes('bought') || desc.includes('sold')) {
            type = 'swap';
          } else if (desc.includes('stake') || desc.includes('unstake')) {
            type = 'stake';
          } else if (desc.includes('received') && (desc.includes('sol') || desc.includes('token'))) {
            type = 'reward';
          } else if (desc.includes('failed') || desc.includes('error')) {
            type = 'failed';
          }

          // Extract token information from description with enhanced regex patterns
          const tokenPatterns = [
            /(\d+\.?\d*)\s+([A-Za-z]{3,20})\s+for/i,        // "99.01 Pepe for"
            /swapped.*?(\d+\.?\d*)\s+([A-Za-z]{2,20})/i,    // "swapped 99.01 Pepe"
            /bought.*?(\d+\.?\d*)\s+([A-Za-z]{2,20})/i,     // "bought 99.01 Pepe"  
            /sold.*?(\d+\.?\d*)\s+([A-Za-z]{2,20})/i,       // "sold 99.01 Pepe"
            /(\d+\.?\d*)\s+([A-Za-z]{2,20})$/i              // "99.01 Pepe" at end
          ];

          for (const pattern of tokenPatterns) {
            const match = description.match(pattern);
            if (match) {
              const [_, amountStr, tokenStr] = match;
              amount = parseFloat(amountStr) || 0;
              tokenSymbol = tokenStr.toUpperCase();
              token = tokenSymbol;
              
              // Enhanced token name mapping including PEPE
              const tokenMap: Record<string, string> = {
                'PEPE': 'Pepe',
                'USDC': 'USD Coin',
                'USDT': 'Tether',
                'WSOL': 'Wrapped SOL', 
                'MSOL': 'Marinade SOL',
                'JUP': 'Jupiter',
                'BONK': 'Bonk',
                'WIF': 'dogwifhat',
                'RAY': 'Raydium',
                'ORCA': 'Orca',
                'SAMO': 'Samoyedcoin',
                'COPE': 'Cope',
                'FIDA': 'Bonfida'
              };
              
              tokenName = tokenMap[tokenSymbol] || tokenSymbol;
              break;
            }
          }
        }

        // Process token transfers for accurate token data
        if (tx.tokenTransfers && tx.tokenTransfers.length > 0) {
          for (const transfer of tx.tokenTransfers) {
            if (transfer.fromUserAccount === address || transfer.toUserAccount === address) {
              if (transfer.tokenAmount && transfer.mint) {
                tokenSymbol = transfer.symbol || tokenSymbol;
                tokenName = transfer.name || tokenName;
                contractAddress = transfer.mint;
                amount = Math.abs(parseFloat(transfer.tokenAmount) || 0);
              }
            }
          }
        }

        // Process native SOL transfers
        if (tx.nativeTransfers && tx.nativeTransfers.length > 0) {
          for (const nativeTransfer of tx.nativeTransfers) {
            if (nativeTransfer.fromUserAccount === address || nativeTransfer.toUserAccount === address) {
              if (token === 'SOL' && nativeTransfer.amount) {
                amount = Math.abs(nativeTransfer.amount / 1e9); // Convert lamports to SOL
              }
            }
          }
        }

        events.push({
          id: `sol_${tx.signature}`,
          type: tx.transactionError ? 'failed' : type,
          description: tx.transactionError ? `${description} (failed)` : description,
          timestamp: tx.timestamp ? tx.timestamp * 1000 : Date.now(),
          hash: tx.signature,
          chain: 'sol' as Chain,
          amount,
          token,
          tokenSymbol,
          tokenName,
          contractAddress,
          fromAddress: undefined,
          toAddress: undefined
        });

      } catch (error) {
        // Fallback for problematic transactions
        console.warn(`Failed to parse transaction ${tx.signature}:`, error);
      }
    }

    // Calculate stats from transaction data
    const successfulTxs = events.filter(e => e.type !== 'failed');
    const swapTxs = events.filter(e => e.type === 'swap');
    const rewardTxs = events.filter(e => e.type === 'reward' || e.type === 'stake');
    
    const totalValue = events.reduce((sum, event) => sum + Math.abs(event.amount || 0), 0);
    const miningRewards = rewardTxs.reduce((sum, event) => sum + (event.amount || 0), 0);
    const solToUsd = 200;
    
    const stats: WalletStats = {
      totalSwaps: swapTxs.length,
      totalValue: Math.round(totalValue * solToUsd * 100) / 100,
      miningRewards: Math.round(miningRewards * 100) / 100,
      pondProStatus: false,
      pondProExpiry: null
    };

    return { events, stats };

  } catch (error) {
    console.error('Error fetching Solana data:', error);
    throw error;
  }
}

