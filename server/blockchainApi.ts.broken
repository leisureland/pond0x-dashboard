export async function fetchPond0xData(identifier: string): Promise<{
  ethAddress?: string;
  solAddress?: string;
  xHandle?: string;
  pondProStatus: boolean;
  pondProExpiry?: Date;
} | null> {
  console.log(`Querying pond0x.com for identifier: ${identifier}`);
  
  try {
    console.log(`Attempting to find pond0x.com API endpoint for: ${identifier}`);
    
    // Try pond0x API patterns found in network tab analysis
    const possibleEndpoints = [
      `https://www.pond0x.com/api/auth/user`,
      `https://www.pond0x.com/api/mining/user`, 
      `https://www.pond0x.com/api/mining/profile`,
      `https://www.pond0x.com/api/user/${encodeURIComponent(identifier)}`,
      `https://www.pond0x.com/api/users/${encodeURIComponent(identifier)}`,
      `https://www.pond0x.com/api/profile/${encodeURIComponent(identifier)}`,
      `https://www.pond0x.com/api/account/${encodeURIComponent(identifier)}`,
      `https://pond0x.com/api/auth/user`,
      `https://pond0x.com/api/mining/user`,
      `https://api.pond0x.com/user/${encodeURIComponent(identifier)}`
    ];

    for (const endpoint of possibleEndpoints) {
      try {
        const controller = new AbortController();
        const timeoutId = setTimeout(() => controller.abort(), 3000);
        
        const response = await fetch(endpoint, {
          method: 'GET',
          headers: {
            'User-Agent': 'Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36',
            'Accept': 'application/json, */*',
            'Referer': 'https://pond0x.com/mining',
            'Origin': 'https://pond0x.com'
          },
          signal: controller.signal
        });
        
        clearTimeout(timeoutId);
        
        if (response.ok) {
          console.log(`Found working pond0x endpoint: ${endpoint}`);
          const data = await response.json();
          console.log(`pond0x API response:`, JSON.stringify(data).substring(0, 200));
          
          // Try to extract wallet addresses from the response
          return {
            xHandle: data.xHandle || data.twitter || data.handle || identifier,
            ethAddress: data.ethAddress || data.ethereum || data.eth_address,
            solAddress: data.solAddress || data.solana || data.sol_address,
            pondProStatus: data.pondProStatus || data.isPondPro || data.pond_pro || false,
            pondProExpiry: data.pondProExpiry ? new Date(data.pondProExpiry) : undefined
          };
        }
      } catch (endpointError) {
        // Continue to next endpoint
        continue;
      }
    }
    
    console.log(`No working API endpoint found for: ${identifier}`);
    return null;
    
  } catch (error) {
    console.warn(`Error querying pond0x.com APIs: ${error}`);
